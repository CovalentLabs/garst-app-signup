{
    "compilerOptions": {
        "target": "ES6",
        "noImplicitAny": false,
        "removeComments": false,
        "preserveConstEnums": true,
        "sourceMap": true,
        "types": [
            "node"
        ]
    },
    "exclude": [
        "node_modules",
        "deps"
    ],
    "awesomeTypescriptLoaderOptions": {
        "useCache": true,
        "useBabel": true
    }
}